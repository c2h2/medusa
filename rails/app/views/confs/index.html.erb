<h1>Listing confs</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @confs.each do |conf| %>
  <tr>
    <td><%= conf.key %></td>
    <td><%= conf.value %></td>
    <td><%= link_to 'Show', conf %></td>
    <td><%= link_to 'Edit', edit_conf_path(conf) %></td>
    <td><%= link_to 'Destroy', conf, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Conf', new_conf_path %>
