<h1>Listing pages</h1>

<table>
  <tr>
    <th>Worker</th>
    <th>Code</th>
    <th>Fn</th>
    <th>Charset</th>
    <th>Title</th>
    <th>Mime</th>
    <th>Etag</th>
    <th>Resp ms</th>
    <th>Length</th>
    <th>State</th>
    <th>Hash</th>
    <th>Lm at</th>
    <th>Exp at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pages.each do |page| %>
  <tr>
    <td><%= page.worker %></td>
    <td><%= page.code %></td>
    <td><%= page.fn %></td>
    <td><%= page.charset %></td>
    <td><%= page.title %></td>
    <td><%= page.mime %></td>
    <td><%= page.etag %></td>
    <td><%= page.resp_ms %></td>
    <td><%= page.length %></td>
    <td><%= page.state %></td>
    <td><%= page.hash %></td>
    <td><%= page.lm_at %></td>
    <td><%= page.exp_at %></td>
    <td><%= link_to 'Show', page %></td>
    <td><%= link_to 'Edit', edit_page_path(page) %></td>
    <td><%= link_to 'Destroy', page, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Page', new_page_path %>
